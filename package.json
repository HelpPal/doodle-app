{
  "name": "doodle-app",
  "version": "1.0.0",
  "description": "Doodle App",
  "main": "public/desktop.js",
  "scripts": {
    "rebuild": "npm rebuild --runtime=electron --target=1.4.13 --disturl=https://atom.io/download/atom-shell --abi=47",
    "build:production": "NODE_ENV=production node_modules/.bin/webpack",
    "build": "node_modules/.bin/webpack",
    "start": "node dev-server.js",
    "build:electron": "BUILD_ENV=electron node_modules/.bin/webpack",
    "start:electron": "electron ./public/desktop.js",
    "start:api": "node ./public/api.js",
    "test": "NODE_ENV=test node_modules/.bin/karma start --single-run --browsers PhantomJS",
    "package:mac": "./node_modules/.bin/build --mac --x64",
    "package:win": "./node_modules/.bin/build --win --x64",
    "tar:mac": "tar czf ./dist/mac/DoodleApp-${npm_package_version}-OSX.tar.gz ./dist/mac/DoodleApp-${npm_package_version}-mac.zip",
    "zip:win": "zip -ryXq9 dist/DoodleApp-${npm_package_version}-Windows.zip dist/DoodleApp\\ Setup\\ ${npm_package_version}.exe",
    "dist:mac": "npm run build && npm run package:mac && npm run tar:mac",
    "dist:win": "npm run build && npm run package:win && npm run zip:win",
    "postinstall": "install-app-deps"
  },
  "build": {
    "appId": "doodleApp",
    "asar": false,
    "mac": {
      "category": "com.jonbrennecke.doodleApp"
    },
    "win": {}
  },
  "author": "Brett Berry, Jon Brennecke",
